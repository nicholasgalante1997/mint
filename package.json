{
  "name": "@couch-gag/mint",
  "version": "1.0.0",
  "description": "A minimal technology blog by Nick Galante.",
  "author": "Nick J Galante <nicholasgalante1997@gmail.com>",
  "type": "module",
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "format": "prettier --write server src webpack",
    "lint:web": "eslint --config .eslintrc.web.cjs \"src/**/*.{js,jsx,mjs,cjs,ts,tsx}\"",
    "lint:web-fix": "eslint --config .eslintrc.web.cjs --fix \"src/**/*.{js,jsx,mjs,cjs,ts,tsx}\"",
    "lint:node": "eslint --config .eslintrc.node.cjs \"server/**/*.{js,jsx,mjs,cjs,ts,tsx}\"",
    "lint:node-fix": "eslint --config .eslintrc.node.cjs --fix \"server/**/*.{js,jsx,mjs,cjs,ts,tsx}\"",
    "lint": "run-p -c lint:node lint:web",
    "lint:fix": "run-p -c lint:node-fix lint:web-fix",
    "summarize-articles": "node scripts/summarize.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all package:clean summarize-articles package:bundleJs package:compileBuildScript package:buildMarkup package:cleanTmp package:copyCss package:copyAssets",
    "package:clean": "rm -rf build",
    "package:compileBuildScript": "webpack --config webpack/webpack.xng.cjs",
    "package:buildMarkup": "node .xng/main.cjs",
    "package:cleanTmp": "rm -rf .xng/",
    "package:bundleJs": "webpack --config webpack/webpack.prod.cjs",
    "package:copyCss": "node src/postcss/index.mjs && cp -R node_modules/heller-2-lite/build/css/* build/web",
    "package:copyAssets": "cp -R src/styles/*.*tf assets/* favicons/* build/web",
    "dev:HOME": "REQUEST_PAGE=\"@mint-home-page\" webpack serve --config webpack/webpack.dev.cjs",
    "dev:BROWSE": "REQUEST_PAGE=\"@mint-browse-page\" webpack serve --config webpack/webpack.dev.cjs",
    "dev:ARTICLE": "REQUEST_PAGE=\"@mint-main-article-page\" webpack serve --config webpack/webpack.dev.cjs",
    "dev:PROFILE": "REQUEST_PAGE=\"@mint-profile-page\" webpack serve --config webpack/webpack.dev.cjs",
    "start": "npx serve build/web",
    "start:server": "node server/pm2.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@iconscout/react-unicons": "^2.0.2",
    "@vercel/analytics": "^1.2.2",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "debounce": "^2.0.0",
    "fuse.js": "^7.0.0",
    "gsap": "^3.12.2",
    "heller-2-lite": "^0.0.21",
    "heller-2-react": "^0.0.8",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.29.4",
    "pino": "^8.16.1",
    "react": "19.0.0-beta-26f2496093-20240514",
    "react-dom": "19.0.0-beta-26f2496093-20240514",
    "react-lottie": "^1.2.4",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "react-rough-notation": "^1.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "sleepydogs": "^1.0.7",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "npm:types-react@beta",
    "@types/react-dom": "npm:types-react-dom@beta",
    "@types/react-lottie": "^1.2.10",
    "@types/react-modal": "^3.16.3",
    "@types/react-syntax-highlighter": "^15.5.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "autocannon": "^7.14.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-compiler": "0.0.0-experimental-c23de8d-20240515",
    "css-loader": "^6.8.1",
    "cssnano": "^7.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-compiler": "0.0.0-experimental-53bb89e-20240515",
    "glob": "^10.3.12",
    "html-webpack-plugin": "^5.5.3",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^10.2.3",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "storybook": "^7.5.3",
    "style-loader": "^3.3.3",
    "typescript": "^5.4.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "overrides": {
    "@types/react": "npm:types-react@beta",
    "@types/react-dom": "npm:types-react-dom@beta"
  }
}
