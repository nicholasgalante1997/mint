{
  "name": "@couch-gag/mint",
  "version": "1.0.0",
  "description": "A minimal technology blog by Nick Galante.",
  "author": "Nick J Galante <nicholasgalante1997@gmail.com>",
  "type": "module",
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "format": "prettier --write server src webpack build.tsx",
    "summarize-articles": "node scripts/summarize.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all package:clean summarize-articles package:bundleJs package:compileBuildScript package:buildMarkup package:cleanTmp package:copyAssets",
    "package:clean": "rm -rf build",
    "package:compileBuildScript": "webpack --config webpack/webpack.build-script.cjs",
    "package:buildMarkup": "node .build-process/main.cjs",
    "package:cleanTmp": "rm -rf .build-process/",
    "package:bundleJs": "webpack --config webpack/webpack.prod.cjs",
    "package:copyAssets": "cp -R node_modules/heller-2-lite/build/css/* src/styles/* assets/* build/web",
    "dev:HOME": "REQUEST_PAGE=\"@mint-home-page\" webpack serve --config webpack/webpack.dev.cjs",
    "dev:BROWSE": "REQUEST_PAGE=\"@mint-browse-page\" webpack serve --config webpack/webpack.dev.cjs",
    "dev:ARTICLE": "REQUEST_PAGE=\"@mint-main-article-page\" webpack serve --config webpack/webpack.dev.cjs",
    "start": "npx serve build/web",
    "start:server": "node server/pm2.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@iconscout/react-unicons": "^2.0.2",
    "@vercel/analytics": "^1.2.2",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "fuse.js": "^7.0.0",
    "gsap": "^3.12.2",
    "heller-2-lite": "^0.0.19",
    "heller-2-react": "^0.0.6",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.29.4",
    "pino": "^8.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-rough-notation": "^1.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "sleepydogs": "^1.0.3",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-syntax-highlighter": "^15.5.10",
    "@types/uuid": "^9.0.7",
    "autocannon": "^7.14.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "html-webpack-plugin": "^5.5.3",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "storybook": "^7.5.3",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
